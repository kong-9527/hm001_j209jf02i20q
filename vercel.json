{
    "version": 2,
    "builds": [
      { "src": "client/package.json", "use": "@vercel/next" },
      { "src": "server/dist/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/dist/index.js" },
      { "src": "/(.*)", "dest": "client/$1" }
    ]
  }